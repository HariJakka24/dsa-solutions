class Solution:
    
    def permute(self, nums: List[int]) -> List[List[int]]:
        result = []
        n = len(nums)
        if n == 1:
            return [nums.copy()]
        for i in range(n):
            a = nums[i]
            perms = self.permute(nums[0:i]+nums[i+1:n])
            for perm in perms:
                perm.append(a)
                result.append(perm)
        return result
    
